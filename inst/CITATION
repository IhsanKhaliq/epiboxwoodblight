bib <- bibentry(
  bibtype = "Manual",
  title = "{epiboxwoodblight}: A research compendium to accompany 'Epidemiology of boxwood blight in western North Carolina and Virginia and validation of boxwood blight infection risk model'",
  author = list(
    person("Ihsanul Khaliq"),
    person("Herve Avenot"),
    person("Anton Baudoin"),
    person("Leonard Coop"),
    person("Chuanxue Hong")
  ),
  year = sub("-.*", "", meta$Date),  # Assuming meta$Date is a string with format "YYYY-MM-DD"
  note = sprintf("R package version %s", meta$Version)
)

















